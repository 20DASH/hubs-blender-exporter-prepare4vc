{
  "gltfExtensionName": "MOZ_hubs_components",
  "gltfExtensionVersion": 3,
  "components": {
    "kit-piece": {
      "properties": {
        "id": {
          "type": "string",
          "default": "$name"
        },
        "name": {
          "type": "string",
          "default": "$name"
        },
        "tags": {
          "type": "array",
          "arrayType": "string",
          "default": "$users_collection.*.name"
        }
      }
    }
  }
}